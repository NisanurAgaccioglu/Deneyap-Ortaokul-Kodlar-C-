#include <iostream>
using namespace std;

int main(){

int sicaklik;

cout<<"Sicaklik degeri giriniz : ";
cin>>sicaklik;

if(sicaklik<0)
    cout<<"Kati"<<endl;

else if(sicaklik <100)
    cout<<"Sivi"<<endl;

else if(sicaklik < 1000)
    cout<<"Gaz"<<endl;

else
    cout<<"Plazma"<<endl;

return 0;
}
